# top-level makefile template for "misc_tools"

SHELL	= /bin/sh

@SET_MAKE@
MAKE_RECUR	= @cf_cv_makeflags@ prefix=$(prefix) DESTDIR=$(DESTDIR)

top_srcdir	= @top_srcdir@
srcdir		= @srcdir@
srcdir_sl	= @srcdir@/
VPATH		= @srcdir@
@SET_MAKE@

prefix		= @prefix@
exec_prefix	= @exec_prefix@

all \
clean \
clobber \
distclean \
install \
uninstall ::
